generator client {
  provider = "prisma-client-js"
  output = "../node_modules/.prisma/client"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model expense {
  itemId Int @id @default(autoincrement())
  date DateTime
  country String 
  city String
  category String
  subcategory String
  description String
  cost Int
  paidOption String
  note String?

}